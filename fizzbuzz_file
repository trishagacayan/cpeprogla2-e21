//fizzbuzz file

#include<iostream>
#include<fstream>
using namespace std;

 main(){

	ifstream input("input.txt");
	int num;
	while(input >> num){
	 	if((num%3||num%5)==0)
			cout << "Fizzbuzz" <<endl;
		
		else if(num%3==0)
		{
			cout <<"Fizz" <<endl;
		}
		 else if (num%5==0)
		{
		
			cout <<"Buzz" << endl;	
		}
		else 
			cout << num <<endl;
	}
	system("pause");
}

