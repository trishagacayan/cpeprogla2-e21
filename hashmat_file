//Hashmat using file handling
#include<iostream>
#include<fstream>
#include<math.h>
#include<iomanip>
 using namespace std;
 
 main(){
 	ifstream subtract("sample.txt");
 	int num1;
 	int num2;
 	int num3;
 	 	cout<<"====================="<<endl;
 		cout <<"      Hashmat"<<endl;
 		cout<<"====================="<<endl;
 	while(subtract>>num1>>num2){
 		cout <<num1<<" - "<<num2<<"= ";
 		cout << abs(num2-num1)<<endl;
	 }
 }
